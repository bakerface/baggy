  },
  "xo": {
    "space": 2,
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.3.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "xo": "^0.14.0"
  }
}
